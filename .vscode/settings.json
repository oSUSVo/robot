{
    "files.associations": {
        "stdio.h": "c"
    },
    "code-runner.executorMap": {
        "c": "cd \"$dir\" && gcc \"$fileName\" -o \"$fileNameWithoutExt\" -lm -lmpfr -lgmp && ./\"$fileNameWithoutExt\""
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true
}